#!/bin/bash

path=$2
dir=`echo "${path%/}"`

filename=$(basename $1)
rootname=$(echo "$filename" |cut -f1 -d '.')
refname="+$rootname+" 

awk '{for (I=1;I<NF;I++) if ($I == ":NET") print $(I+1)}' $1 > $dir/.tmp.net

cat $1 | sed "s/(/\ /g;s/)/\ /g;s/  */\ /g;s/\*/+/g" > $dir/.tmp

IFS=$' ' read -r -a arr < $dir/.tmp

for item in "${arr[@]}"; 
do
	
if [ "${item: -1}" = "+" ] && [ "${item:0:1}" = "+" ] && [ "$refname" != "$item" ]

then
 echo $item | sed "s/+/\*/g" >> $dir/.tmp.var
fi

done

rm $dir/.tmp
