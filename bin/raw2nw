#!/bin/bash

##     |-------------- SBCL --------------|                                      |--- with-label ---|
##     | one line   | reduce space        | rep \ ! to :      | rep ! to :       | rem \ -          | rep \  to ,      | add ; at the end
cat $1 | tr -d '\n' | sed -e 's/  */\ /g' | sed -e 's/ !/:/g' | sed -e 's/!/:/g' | sed -e 's/ -//g' | sed -e 's/ /,/g' | awk '{print $0";"}' > $2
rm $1